{
  "name": "blog.sboh.dev",
  "description": "Gatsby blog starter kit with beautiful and simple design",
  "version": "2002.12.14",
  "license": "MIT",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint ./src --fix --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
